[scheduling]
    initial cycle time = 20140101
    final cycle time = 20140104
    [[dependencies]]
        graph = "cold_foo"
        [[[ T00 ]]]
            graph = "foo_midnight[-P1D] & cold_foo => foo_midnight"
        [[[ T06 ]]]
            graph = "foo_dawn[-P1D] & cold_foo => foo_dawn"
[runtime]
    [[root]]
        command scripting = sleep 1
