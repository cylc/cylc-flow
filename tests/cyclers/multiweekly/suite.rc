[cylc]
    UTC mode = True
    [[reference test]]
        live mode suite timeout = 3.0
[scheduling]
    initial cycle point = 1000W011
    final cycle point = 1000W064
    [[dependencies]]
        [[[ P1W ]]]
            graph = "foo[-P1W] => foo => bar"
        [[[ P3W ]]]
            graph = "baz[-P3W] => baz => qux"
[runtime]
    [[root]]
        command scripting = true
