        inherit = FAM
        command scripting = echo I am $TITLE 1
    [[member_2]]
        inherit = FAM
        command scripting = echo I am $TITLE 2
    [[member_3]]
        inherit = FAM
        command scripting = echo I am $TITLE 3
    [[member_4]]
        inherit = FAM
        command scripting = echo I am $TITLE 4