#!jinja2
[cylc]
    UTC mode = False
    [[reference test]]
        live mode suite timeout = PT10M

[scheduling]
    [[graph]]
        R1=t1

[runtime]
    [[t1]]
        script=test -z "${TZ:-}"
        [[[job]]]
            execution time limit = PT1M
        [[[remote]]]
            host={{environ["CYLC_TEST_HOST"]}}
