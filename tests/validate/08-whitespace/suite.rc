#!jinja2   
    
{% set HELLO=False %}    
[  scheduling   ]    
    initial cycle time = 2014010100
    [[  special tasks  ]]    
        sequential = foo , bar ,  baz   
        start-up =   \
            foo {% if HELLO %}   ,    \
            bar    
            {% endif %}   
    [[   dependencies  ]]   
        [[[ 0, 12  ]]]   
            graph="""   
        a => b   
        foo => bar &  baz  """   
        
   [  runtime   ]    
    [[  foo,bar   ,  a,  b  ]]   
%include "inc.rc"   

