#!jinja2
[scheduler]
    UTC mode=True
    cycle point format = %Y
    [[events]]
        abort on stall = True
        abort on inactivity timeout = True
        inactivity timeout = PT3M
[scheduling]
    initial cycle point = 2016
    final cycle point = 2031
    [[graph]]
        P1Y=t1 & t2
[runtime]
    [[t1, t2]]
        script = sleep $((RANDOM % 2 + 6))
        [[[job]]]
            execution time limit = PT25S
