[cylc]
   [[reference test]]
       required run mode = live
       live mode suite timeout = 0.5 # minutes
[scheduling]
    [[dependencies]]
        graph = """a => b
                   a => c
                   c => d"""
[runtime]
    [[a]]
        script = "cylc depend $CYLC_SUITE_NAME d.1 b.1; sleep 10"
    [[b,c,d]]
        script = true
