[cylc]
   [[reference test]]
       required run mode = live
       live mode suite timeout = 0.5 # minutes
[scheduling]
    [[dependencies]]
        graph = """a => b
                   a => c
                   c => d"""
[runtime]
    [[a]]
        command scripting = "cylc depend $CYLC_SUITE_NAME d.0 b.0; sleep 10"
    [[b,c,d]]
        command scripting = true
