[cylc]
    UTC mode = True
    [[event hooks]]
        timeout = 0.1
        abort on timeout = True
[scheduling]
    initial cycle time = 20100101T00
    final cycle time = 20100105T00
    [[dependencies]]
        [[[PT3H]]]
            graph = "foo => bar"
[runtime]
    [[foo]]
        command scripting = false
    [[bar]]
        command scripting = true
