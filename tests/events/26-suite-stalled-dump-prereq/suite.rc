[cylc]
    UTC mode = True # Ignore DST
    [[event hooks]]
        abort on stalled = true
    [[reference test]]
        allow task failures = true
        live mode suite timeout=PT1M
[scheduling]
    initial cycle point = 20100101T0000Z
    [[dependencies]]

        [[[T00, T06, T12, T18]]]
            graph = foo[-PT6H] & bar[-PT6H] => foo => bar => qux

        [[[T12]]]
            graph = qux[-PT6H] => baz
[runtime]
    [[root]]
        script = true
    [[foo,baz, qux]]
    [[bar]]
        script = false
