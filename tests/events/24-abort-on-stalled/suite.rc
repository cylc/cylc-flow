[cylc]
    UTC mode = True # Ignore DST
    [[event hooks]]
        abort on stalled = true
    [[reference test]]
        allow task failures = true
        live mode suite timeout=PT1M
[scheduling]
    [[dependencies]]
        graph = foo => bar => baz

[runtime]
    [[foo,baz]]
        script = true
    [[bar]]
        script = false
