#!jinja2
[general]
    [[events]]
        handlers = echo %(rubbish)s
        handler events = startup
{% if ABORT is defined %}
        abort if startup handler fails = True
{% endif %}
    [[reference test]]
        live mode suite timeout=PT1M

[scheduling]
    [[graph]]
        R1=t1

[runtime]
    [[t1]]
        script=true
