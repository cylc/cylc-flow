[cylc]
    cycle point time zone = +01
    [[reference test]]
        required run mode = live
        live mode suite timeout = 1.0 # minutes
[scheduling]
    initial cycle point = 20100101T00
    final cycle point   = +P5D
    [[dependencies]]
        [[[T00]]]
            graph = "foo[-P1D] => foo"
[runtime]
    [[foo]]
        command scripting = true
