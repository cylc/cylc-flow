[meta]
    title = A suite that should only succeed in dummy mode.
[general]
    [[events]]
        abort on stalled = True
    [[reference test]]
        required run mode = dummy
[scheduling]
    [[graph]]
        R1 = foo:x => bar
[runtime]
    [[root]]
        [[[simulation]]]
            default run length = PT0S
            time limit buffer = PT1M
    [[bar]]
    [[foo]]
        script = false
        [[[outputs]]]
            x = "the quick brown fox"
