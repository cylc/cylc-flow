[cylc]
   UTC mode = True
   [[reference test]]
       required run mode = live
       live mode suite timeout = PT1M
[scheduling]
    initial cycle point = 20100101T00
    final cycle point = 20100102T00
    [[dependencies]]
        PT6H = "a[-PT6H] & b[-PT6H] => a => b"
[runtime]
    [[a, b]]
        script = "sleep 1"
