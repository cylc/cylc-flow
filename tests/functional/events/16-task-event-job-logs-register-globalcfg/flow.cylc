#!jinja2
[meta]
    title=Task Event Job Log Retrieve

[scheduling]
    [[graph]]
        R1=t1

[runtime]
    [[t1]]
        init-script = cylc__job__disable_fail_signals ERR EXIT
        script="""
cylc__job__wait_cylc_message_started
mv "$0.out" "$0.out.keep"
mv "$0.err" "$0.err.keep"
cylc message 'succeeded' >>"$0.out.keep"
sleep 5
cp -p "$0.out.keep" "$0.out"
cp -p "$0.err.keep" "$0.err"
"""
