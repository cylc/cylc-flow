# Should stall at runhead limit with incomplete foo
[scheduler]
   [[events]]
      abort on stall = True
[scheduling]
   cycling mode = integer
   initial cycle point = 1
   runahead limit = P1
   [[graph]]
      P1 = "foo:x & foo:y => bar"
[runtime]
   [[foo]]
     script = "cylc message x"
     [[[outputs]]]
       x = x
       y = y
   [[bar]]
