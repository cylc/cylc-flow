[scheduling]
    [[queues]]
        [[[my_queue]]]
            limit = 1
            members = METASYNTACTIC
    [[graph]]
        R1 = "foo:start => bar"
[runtime]
    [[METASYNTACTIC]]
    [[foo]]
        inherit = METASYNTACTIC
        script = """
            cylc__job__wait_cylc_message_started
            cylc trigger $CYLC_WORKFLOW_ID bar 1
        """
    [[bar]]
        inherit = METASYNTACTIC
        script = true
