# only the t1:hello output is generated
[scheduler]
    [[events]]
        abort on inactivity = True
        abort on stall = True
        inactivity = PT1M
[scheduling]
    [[graph]]
        R1 = """
t1:hello => t2
t1:greet? => t3
"""
[runtime]
    [[t1]]
        script = cylc message 'hello'
        [[[outputs]]]
            hello = hello
            greet = greeting
    [[t2, t3]]
        script = true
