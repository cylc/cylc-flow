# only the t1:hello output is generated
[scheduler]
    [[events]]
        abort on inactivity timeout = True
        abort on stall timeout = True
        stall timeout = PT0S
        inactivity timeout = PT1M
[scheduling]
    [[graph]]
        R1 = """
            t1:hello => t2
            t1:greet? => t3
        """
[runtime]
    [[t1]]
        script = cylc message 'hello'
        completion = succeeded and hello
        [[[outputs]]]
            hello = hello
            greet = greeting
    [[t2, t3]]
        script = true
