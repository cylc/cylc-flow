        script = """
cylc__job__wait_cylc_message_started
cylc message -p WARNING "${CYLC_SUITE_NAME}" "${CYLC_TASK_JOB}" \
    "Warn this" "INFO: Greeting" - <<'__MESSAGES__'
Warn that

DEBUG: Remove stuffs such as
badness
slowness
and other incorrectness.

CUSTOM: whatever
