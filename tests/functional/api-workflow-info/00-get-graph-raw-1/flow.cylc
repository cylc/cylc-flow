[scheduling]
    [[graph]]
        R1 = t1 => t2 & t3
[runtime]
    [[t1]]
        script = """
            cylc client \
                "$CYLC_WORKFLOW_ID" \
                get_graph_raw \
                >"${CYLC_WORKFLOW_RUN_DIR}/ctb-get-graph-raw.out" \
                <<__HERE__
            {
                "start_point_str": "1",
                "stop_point_str": "1",
                "grouping": ["<all>"]
            }
            __HERE__
        """
    [[T]]
        script = true
    [[t2, t3]]
        inherit = T
