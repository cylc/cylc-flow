[scheduler]
    UTC mode = True
    allow implicit tasks = True
    [[events]]
        abort on stalled = True
        stalled timeout = PT30S
        abort on stalled timeout = True
[scheduling]
    initial cycle point = 20100101T00
    final cycle point = 20100105T00
    [[graph]]
        # T00, T07, T14, ...
        # oops makes bar spawn as waiting
        PT7H = "foo & oops => bar"
        # T00, T12, T18...
        T00, T12, T18 = "foo"
        # T04...
        T04 = "run_ok"
        # T05...
        T05 = "run_ok_2"
[runtime]
    [[foo, fail]]
        script = false
    [[bar]]
        script = true
