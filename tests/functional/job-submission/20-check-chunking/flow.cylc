#!Jinja2

[scheduler]
    [[events]]
        abort on inactivity timeout = True
        abort on stall = True
        inactivity timeout = PT10M

[task parameters]
    p = 1..5

[scheduling]
    [[graph]]
        R1 = t1<p> => fin

[runtime]
    [[t1<p>]]
        platform = {{ CYLC_TEST_PLATFORM }}
    [[fin]]
