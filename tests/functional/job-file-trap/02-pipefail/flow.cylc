#!jinja2
[scheduler]
   [[events]]
       abort on stall timeout = True
       stall timeout = PT0S
       expected task failures = t1.1, t2.1

[scheduling]
    [[graph]]
        R1=t1 & t2

[runtime]
    [[t1]]
        script=false|cat
    [[t2]]
        # Trigger SIGPIPE signal exit.
        script=yes|true
