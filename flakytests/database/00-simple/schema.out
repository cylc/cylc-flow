CREATE TABLE broadcast_events ( time TEXT, change TEXT, point TEXT, namespace TEXT, "key" TEXT, value TEXT);
CREATE TABLE broadcast_states ( point TEXT NOT NULL, namespace TEXT NOT NULL, "key" TEXT NOT NULL, value TEXT, PRIMARY KEY (point, namespace, "key"));
CREATE TABLE broadcast_states_checkpoints ( id INTEGER NOT NULL, point TEXT NOT NULL, namespace TEXT NOT NULL, "key" TEXT NOT NULL, value TEXT, PRIMARY KEY (id, point, namespace, "key"));
CREATE TABLE checkpoint_id ( id INTEGER NOT NULL, time TEXT, event TEXT, PRIMARY KEY (id));
CREATE TABLE inheritance ( namespace TEXT NOT NULL, inheritance TEXT, PRIMARY KEY (namespace));
CREATE TABLE suite_params ( "key" TEXT NOT NULL, value TEXT, PRIMARY KEY ("key"));
CREATE TABLE suite_params_checkpoints ( id INTEGER NOT NULL, "key" TEXT NOT NULL, value TEXT, PRIMARY KEY (id, "key"));
CREATE TABLE suite_template_vars ( "key" TEXT NOT NULL, value TEXT, PRIMARY KEY ("key"));
CREATE TABLE task_action_timers ( cycle TEXT NOT NULL, name TEXT NOT NULL, ctx_key TEXT NOT NULL, ctx TEXT, delays TEXT, num INTEGER, delay TEXT, timeout TEXT, PRIMARY KEY (cycle, name, ctx_key));
CREATE TABLE task_events ( name TEXT, cycle TEXT, time TEXT, submit_num INTEGER, event TEXT, message TEXT);
CREATE TABLE task_jobs ( cycle TEXT NOT NULL, name TEXT NOT NULL, submit_num INTEGER NOT NULL, is_manual_submit INTEGER, try_num INTEGER, time_submit TEXT, time_submit_exit TEXT, submit_status INTEGER, time_run TEXT, time_run_exit TEXT, run_signal TEXT, run_status INTEGER, user_at_host TEXT, batch_sys_name TEXT, batch_sys_job_id TEXT, PRIMARY KEY (cycle, name, submit_num));
CREATE TABLE task_late_flags ( cycle TEXT NOT NULL, name TEXT NOT NULL, value INTEGER, PRIMARY KEY (cycle, name));
CREATE TABLE task_outputs ( cycle TEXT NOT NULL, name TEXT NOT NULL, outputs TEXT, PRIMARY KEY (cycle, name));
CREATE TABLE task_pool ( cycle TEXT NOT NULL, name TEXT NOT NULL, spawned INTEGER, status TEXT, is_held INTEGER, PRIMARY KEY (cycle, name));
CREATE TABLE task_pool_checkpoints ( id INTEGER NOT NULL, cycle TEXT NOT NULL, name TEXT NOT NULL, spawned INTEGER, status TEXT, is_held INTEGER, PRIMARY KEY (id, cycle, name));
CREATE TABLE task_states ( name TEXT NOT NULL, cycle TEXT NOT NULL, time_created TEXT, time_updated TEXT, submit_num INTEGER, status TEXT, PRIMARY KEY (name, cycle));
CREATE TABLE task_timeout_timers ( cycle TEXT NOT NULL, name TEXT NOT NULL, timeout REAL, PRIMARY KEY (cycle, name));
CREATE TABLE xtriggers ( signature TEXT NOT NULL, results TEXT, PRIMARY KEY (signature));